<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="View.AddItemController"
            prefWidth="400">

    <VBox AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.bottomAnchor="10" AnchorPane.rightAnchor="10" spacing="10">
        <TextField fx:id="addItemField"/>
        <HBox alignment="CENTER_RIGHT" spacing="10">
            <Button fx:id="addButton" text="Add" />
            <Button fx:id="cancelButton" text="Cancel"/>
        </HBox>
    </VBox>

</AnchorPane>
